(function(){function getWindowWidth(){return $(window).width()}function hidePreloader(){$(".jp-ld-preloader").fadeOut()}function isMobile(){if(navigator.userAgent.match(/Android/i)||navigator.userAgent.match(/webOS/i)||navigator.userAgent.match(/iPhone/i)||navigator.userAgent.match(/iPad/i)||navigator.userAgent.match(/iPod/i)||navigator.userAgent.match(/BlackBerry/i)||navigator.userAgent.match(/Windows Phone/i)){return true}else{return false}}$(window).bind("load",function(){hidePreloader()});$('label[for="vt-agree-all"], #vt-agree-all').bind("click",function(){setTimeout(function(){if($("#vt-agree-all").prop("checked")===true){checkActiveAll(true)}else{checkActiveAll(false)}},100)});function checkActiveAll(bool){$('input[type="checkbox"]').prop("checked",bool)}function autoHypenPhone(str){str=str.replace(/[^0-9]/g,"");var tmp="";if(str.length<4){return str}else if(str.length<7){tmp+=str.substr(0,3);tmp+="-";tmp+=str.substr(3);return tmp}else if(str.length<11){tmp+=str.substr(0,3);tmp+="-";tmp+=str.substr(3,3);tmp+="-";tmp+=str.substr(6);return tmp}else{tmp+=str.substr(0,3);tmp+="-";tmp+=str.substr(3,4);tmp+="-";tmp+=str.substr(7);return tmp}return str}function applyPhoneNumberEvent(){var val=$(this).val().trim();$(this).val(autoHypenPhone(val))}var inputField=$('input[type="tel"]');inputField.bind("keydown keyup",applyPhoneNumberEvent);window.getWindowWidth=getWindowWidth;window.isMobile=isMobile})();